{
  "expo": {
    "name": "Nastia-Chef",
    "slug": "Nastia-Chef",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nakonsta.NastiaChef"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Quicksand/Quicksand-Medium.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e1a7c577-e8a9-4541-b052-52883e2b0abb"
      }
    }
  }
}
